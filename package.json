{
	"name": "histoire-sveltekit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "svelte-kit sync && vite dev",
		"build": "svelte-kit sync && vite build",
		"preview": "svelte-kit sync && vite preview",
		"story": "svelte-kit sync && histoire dev",
		"story:build": "svelte-kit sync && histoire build",
		"story:preview": "svelte-kit sync && histoire preview",
		"test": "svelte-kit sync && vitest run",
		"test:watch": "svelte-kit sync && vitest watch",
		"coverage": "svelte-kit sync && vitest run --coverage",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@histoire/controls": "^0.11.0",
		"@histoire/plugin-svelte": "^0.11.0",
		"@sveltejs/adapter-auto": "^1.0.0-next.80",
		"@sveltejs/kit": "^1.0.0-next.507",
		"@sveltejs/vite-plugin-svelte": "^1.0.8",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.1",
		"@typescript-eslint/eslint-plugin": "^5.39.0",
		"@typescript-eslint/parser": "^5.39.0",
		"autoprefixer": "^10.4.12",
		"c8": "^7.12.0",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"histoire": "^0.11.0",
		"jsdom": "^20.0.1",
		"plop": "^3.1.1",
		"postcss": "^8.4.17",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.1",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.4.0",
		"typescript": "^4.8.4",
		"vite": "^3.1.4",
		"vitest": "latest"
	},
	"type": "module"
}
